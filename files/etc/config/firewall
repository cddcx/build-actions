
config defaults
	option input 'REJECT'
	option output 'ACCEPT'
	option forward 'REJECT'
	option synflood_protect '1'

config zone
	option name 'lan'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'ACCEPT'
	list network 'lan'

config zone
	option name 'IPTV'
	option input 'ACCEPT'
	option output 'ACCEPT'
	option forward 'REJECT'
        list network 'IPTV'

config nat
	option name 'ab'
	list proto 'all'
	option src '*'
	option target 'SNAT'
	option snat_ip '192.168.3.2'

